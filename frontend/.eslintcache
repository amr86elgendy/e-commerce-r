)\r\n  }\r\n\r\n  const handleTooltip = () => {\r\n    const added = cart.some((p) => p._id === product._id)\r\n    if (added) return 'added'\r\n    else return 'Click to add'\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <div className='col-md-7'>\r\n        <Carousel showArrows={true} autoPlay infiniteLoop>\r\n          {images && images.map((i) => <img src={i.url} key={i.public_id} />)}\r\n        </Carousel>\r\n        <Tabs type='card'>\r\n          <Tabs.TabPane tab='Description' key='1'>\r\n            {description && description}\r\n          </Tabs.TabPane>\r\n          <Tabs.TabPane tab='More' key='2'>\r\n            Call use on xxxx xxx xxx to learn more about this product.\r\n          </Tabs.TabPane>\r\n        </Tabs>\r\n      </div>\r\n\r\n      <div className='col-md-5'>\r\n        <h2 className='bg-info p-3'>{title}</h2>\r\n        {ratings && ratings.length > 0 ? (\r\n          ratingAverage(product)\r\n        ) : (\r\n          <div className='text-center pt-1 pb-3'>No rating yet</div>\r\n        )}\r\n        <Card\r\n          actions={[\r\n            <Tooltip title={handleTooltip}>\r\n              <a onClick={handleAddToCart}>\r\n                <ShoppingCartOutlined className='text-danger' /> <br /> Add to\r\n                Cart\r\n              </a>\r\n            </Tooltip>,\r\n            <a onClick={handleAddToWishlist}>\r\n              {user && user.wishlist && user.wishlist.includes(_id) ? (\r\n                <>\r\n                  <HeartFilled className='text-danger' /> <br /> Added to\r\n                  Wishlist\r\n                </>\r\n              ) : (\r\n                <>\r\n                  <HeartOutlined className='text-danger' /> <br /> Add to\r\n                  Wishlist\r\n                </>\r\n              )}\r\n            </a>,\r\n            <RatingModal>\r\n              <StarRating\r\n                name={_id}\r\n                numberOfStars={5}\r\n                rating={star}\r\n                changeRating={handleRating}\r\n                isSelectable={true}\r\n                starRatedColor='red'\r\n              />\r\n            </RatingModal>,\r\n          ]}\r\n        >\r\n          <ProductsItems product={product} />\r\n        </Card>\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default SingleProduct\r\n","D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\CreateProductForm.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\Typeeffect.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\home\\NewArrivals.js",["353"],"import React, { useEffect, useState } from \"react\";\nimport { getProductsAndSort, getProductsCount } from \"../../functions/product\";\nimport ProductCard from \"../products/ProductCard\";\nimport LoadingCard from \"../helpers/LoadingCard\";\nimport { Pagination } from \"antd\";\n\nconst NewArrivals = () => {\n  const [products, setProducts] = useState([]);\n  const [loading, setLoading] = useState(false);\n  const [productsCount, setProductsCount] = useState(0);\n  const [page, setPage] = useState(1);\n\n  useEffect(() => {\n    loadAllProducts();\n  }, [page]);\n\n  useEffect(() => {\n    getProductsCount().then((res) => setProductsCount(res.data));\n  }, []);\n\n  const loadAllProducts = () => {\n    setLoading(true);\n    // sort, order, limit\n    getProductsAndSort(\"createdAt\", \"desc\", page).then(({data}) => {\n      setProducts(data);\n      setLoading(false);\n    });\n  };\n\n  return (\n    <>\n      <div className=\"container\">\n        {loading ? (\n          <LoadingCard count={3} />\n        ) : (\n          <div className=\"row\">\n            {products.map((product) => (\n              <div key={product._id} className=\"col-md-4\">\n                <ProductCard product={product} />\n              </div>\n            ))}\n          </div>\n        )}\n      </div>\n      <div className=\"row\