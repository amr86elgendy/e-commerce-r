> setPage(value)}\n          />\n        </nav>\n      </div>\n    </>\n  );\n};\n\nexport default BestSellers;\n","D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\UpdateProductForm.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\CategoryForm.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\category\\CategoriesList.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\LocalSearch.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\sub\\SubsList.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\products\\ProductCard.js",["355","356","357"],"import React from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { Card, Tooltip } from 'antd';\r\nimport { EyeOutlined, ShoppingCartOutlined } from '@ant-design/icons';\r\nimport { Link } from 'react-router-dom';\r\nimport { ratingAverage } from '../../functions/rating';\r\n\r\nconst ProductCard = ({ product }) => {\r\n  const { images, title, description, slug, ratings, price, quantity } = product;\r\n  \r\n  const dispatch = useDispatch();\r\n  const { cart, user } = useSelector(state => ({...state}));\r\n  \r\n  const handleAddToCart = () => {\r\n    let cart = localStorage.getItem('cart')\r\n      ? JSON.parse(localStorage.getItem('cart'))\r\n      : [];\r\n    let alreadyExist = cart.some((p) => p._id === product._id);\r\n    if (!alreadyExist) {\r\n      cart.push({ ...product, count: 1 });\r\n      localStorage.setItem('cart', JSON.stringify(cart));\r\n\r\n      dispatch({\r\n        type: 'ADD_TO_CART',\r\n        payload: cart\r\n      });\r\n\r\n      dispatch({\r\n        type: 'SET_SIDEBAR',\r\n        payload: true\r\n      });\r\n    }\r\n  };\r\n\r\n  const handleTooltip = () => {\r\n    const added = cart.some((p) => p._id === product._id);\r\n    if (added) return 'added'\r\n    if (quantity === 0) return 'Out Of Stock'\r\n    else return 'Click to add'\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {ratings && ratings.length > 0 ? (\r\n        ratingAverage(product)\r\n      ) : (\r\n        <div className='text-center pt-1 pb-3'>No rating yet</div>\r\n      )}\r\n      <Card\r\n        cover={\r\n          <img\r\n            src={images && images.length ? images[0].url : ''}\r\n            style={{ height: '150px', objectFit: 'cover' }}\r\n            className='p-1'\r\n          />\r\n        }\r\n        actions={[\r\n          <Link to={`/product/${slug}`}>\r\n            <EyeOutlined className='text-info' /> <br /> View Product\r\n          </Link>,\r\n          <Tooltip title={handleTooltip}>\r\n            <a onClick={handleAddToCart} disabled={quantity === 0}>\r\n            <ShoppingCartOutlined className='text-danger' /> <br /> Add to Cart\r\n          </a>,\r\n          </Tooltip>\r\n        ]}\r\n      >\r\n        <Card.Meta\r\n          title={`${title} - $${price}`}\r\n          description={`${description && description.substring(0, 40)}...`}\r\n        />\r\n      </Card>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductCard;\r\n","D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\helpers\\FileUpload.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\layout\\Search.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\user\\Invoice.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\functions\\order.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\layout\\UserNav.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\user\\PaymentInfo.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\products\\ProductCartItem.js",["358","359"],"import Re