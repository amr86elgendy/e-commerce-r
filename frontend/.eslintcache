         onChange={(e) => {\r\n            setCoupon(e.target.value)\r\n            setDiscountError('')\r\n          }}\r\n          value={coupon}\r\n          type='text'\r\n          className='form-control'\r\n        />\r\n        <button onClick={applyDiscountCoupon} className='btn btn-primary mt-2'>\r\n          Apply\r\n        </button>\r\n        <br />\r\n        {discountError && <p className='bg-danger p-2'>{discountError}</p>}\r\n      </div>\r\n\r\n      <div className='col-md-6'>\r\n        <h4>Order Summary</h4>\r\n        <hr />\r\n        <p>Products {order && order.products ? order.products.length : 0}</p>\r\n        <hr />\r\n        {order &&\r\n          order.products &&\r\n          order.products.map((p, i) => (\r\n            <div key={i}>\r\n              <p>\r\n                {p.product.title} ({p.color}) x {p.count} ={' '}\r\n                {p.product.price * p.count}\r\n              </p>\r\n            </div>\r\n          ))}\r\n        <hr />\r\n        <p>Total Price: $ {order ? order.totalPrice : 0}</p>\r\n        {totalAfterDiscount > 0 && (\r\n          <p className='bg-success p-2'>\r\n            Discount Applied: Total Payable: ${totalAfterDiscount}\r\n          </p>\r\n        )}\r\n        <div className='row'>\r\n          <div className='col-md-6'>\r\n            {cashOnDelivery ? (\r\n              <button\r\n                className='btn btn-primary'\r\n                disabled={!addressSaved || !order}\r\n                onClick={handleCashOrder}\r\n              >\r\n                Place Order\r\n              </button>\r\n            ) : (\r\n              <button\r\n                className='btn btn-primary'\r\n                disabled={!addressSaved || !order}\r\n                onClick={() => history.push('/payment')}\r\n              >\r\n                Place Order\r\n              </button>\r\n            )}\r\n          </div>\r\n\r\n          <div className='col-md-6'>\r\n            <button\r\n              className='btn btn-primary'\r\n              disabled={!order}\r\n              onClick={handleEmptyCart}\r\n            >\r\n              Empty Cart\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Checkout\r\n","D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\layout\\Header.js",[],"D:\\Web-Design-Development\\Node-JS\\E-Commerce-Ryan\\frontend\\src\\components\\layout\\Sidebar.js",["348"],"import React from 'react';\r\nimport { Drawer } from \"antd\";\r\nimport { useSelector, useDispatch } from \"react-redux\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nconst Sidebar = () => {\r\n  const dispatch = useDispatch();\r\n  const { sidebar, cart } = useSelector((state) => ({ ...state }));\r\n\r\n  const imageStyle = {\r\n    width: \"100%\",\r\n    height: \"50px\",\r\n    objectFit: \"cover\",\r\n  };\r\n\r\n  return (\r\n    <Drawer\r\n      className=\"text-center\"\r\n      title={`Cart / ${cart.length} Product`}\r\n      placement=\"right\"\r\n      closable={false}\r\n      onClose={() => {\r\n        dispatch({\r\n          type: \"SET_SIDEBAR\",\r\n          payload: false,\r\n        });\r\n      }}\r\n      visible={sidebar}\r\n    >\r\n      {cart.map((p) => (\r\n        <div key={p._id} className=\"row\">\r\n          <div className=\"col\">\r\n            {p.images[0] && (\r\n              <>\r\n                <img src={p.images[0].url} style={imageStyle} />\r\n                <p className=\"text-center bg-secondary text-light\">\r\n                  {p.title} x {p.count}\r\n                </p>\r\n              </>\r\n            )}\r\n          </div>\r\n        </div>\r\n      ))}\r\n\r\n      <Link to=\"/cart\">\r\n        <button\r\n          onClick={() =>\r\n            dispatch({\r\n              type: \"SET_SIDEBAR\",\r\n              payload: false,\r\n            })\r\n          }\r\n          className=\"text-center btn